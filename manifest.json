{
  "name": "Page Bubble",
  "description": "An AI that lives on a webpage",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "identity"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["jquery-ui.min.css", "jquery-ui.structure.min.css", "jquery-ui.theme.min.css"],
      "js": ["underscore-min.js", "socket.io.js", "socket.service.js", "jquery-2.2.0.min.js", "jquery-ui.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Wake up the webpage AI!"
  },
  "key": "",
  "manifest_version": 2,
  "oauth2": {
    "client_id": "640019354689-cfm9mrsfvic1fp3fddej2bm9plghtb1f.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  }
}
